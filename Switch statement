#include<stdio.h>
int main()
{
int ch=0;
printf("Enter your choice:\n0.case 0\n1.case 1");
scanf("%d",&ch);
switch(ch)
{
   case 0:
     printf("The given number is present in the list");
     break; 
    case 1:
     printf("The given number is not present in the list");
     break;
    default :
     printf("you have entered a wrong choice");   
}
}


